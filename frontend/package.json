{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "lint": "ng lint",
    "lint:json": "eslint \"src/**/*.{ts,html}\" -f json -o eslint-report.json",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:staging": "ng serve --configuration=staging --proxy-config proxy.conf.json",
    "configure": "node set-env.js",
    "build": "npm run configure && ng build --configuration production",
    "build:staging": "ng build --configuration=staging",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "serve:ssr:frontend": "node dist/frontend/server/server.mjs",
    "generate:api-types": "openapi-typescript http://localhost:8080/v3/api-docs -o src/app/types/api.types.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.5",
    "@supabase/supabase-js": "^2.75.0",
    "express": "^4.18.2",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/angular": "^15.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.18.0",
    "angular-eslint": "20.3.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "openapi-typescript": "^7.10.1",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.4.2",
    "typescript-eslint": "8.40.0"
  }
}
